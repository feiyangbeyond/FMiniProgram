<!--pages/run/run.wxml-->

 <view class="swiper" >
    <swiper style="height:500rpx;" indicator-dots='true' indicator-color='red' autoplay='true'>
    <swiper-item  catchtouchmove="stopTouchMove">
      <image style='height:100%;;width:100%;' src='../../pics/2021.png'></image>
    </swiper-item>
  </swiper>
</view>
<navigator class="intr-text" url="/pages/run-sm/run-sm" >使用方法</navigator>
<view class="kuang">
<input bindinput="getzh" type="text" placeholder="请输入账号" value="{{zhanghao}}"></input>
<input bindinput="getmm" type="password" placeholder="请输入密码" value="{{mima}}"></input>
<input bindinput="getbs" type="number" placeholder="请输入步数(最多98000)" value="{{bushu}}"></input>
<button class="emptyBtn" bindtap="getrun">确定</button>
</view>

<view class="shuchu">
<text class="shuchuText">{{shuchu}}</text>
</view>